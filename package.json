{
  "name": "ezd-app-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server -d --colors --hot --inline --progress --port 8080",
    "bundle": "NODE_ENV=production webpack --colors --progress",
    "lib": "rm -rf ./lib && babel src --out-dir lib",
    "build": "npm run lint && npm run lib && npm run bundle"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
